{
  "name": "salte-angular-hateoas",
  "version": "0.0.0-semantically-released.0",
  "description": "The simple way to consume HATEOAS enabled REST APIs with AngularJS",
  "main": "dist/salte-angular-hateoas.js",
  "contributors": [
    "Ceci <admin@cecilias.me>"
  ],
  "keywords": [
    "angular",
    "hypermedia",
    "hateoas",
    "http"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/salte-io/salte-angular-hateoas.git"
  },
  "scripts": {
    "lint": "eslint src/**/*.js tests/**/*.js",
    "test": "karma start --single-run",
    "test:sauce": "karma start karma.ci.conf.js",
    "tdd": "karma start",
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "webpack --progress --colors -d",
    "build:prod": "webpack --progress --colors -p",
    "report-coverage": "coveralls < coverage/lcov.info",
    "semantic-release": "semantic-release pre && npm run build:prod && npm publish && semantic-release post"
  },
  "license": "MIT",
  "peerDependencies": {
    "angular": "^1.5.0"
  },
  "devDependencies": {
    "angular": "^1.6.0",
    "angular-mocks": "^1.5.8",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.18.0",
    "coveralls": "^2.11.14",
    "deindent": "^0.1.0",
    "eslint": "^3.8.1",
    "eslint-config-google": "^0.7.0",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-jasmine": "^2.6.2",
    "eslint-plugin-mocha": "^4.8.0",
    "isparta-loader": "^2.0.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sauce-launcher": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.1",
    "phantomjs": "^2.1.7",
    "semantic-release": "^6.3.2",
    "webpack": "^2.2.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
